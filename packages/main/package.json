{
  "name": "material-template",
  "version": "0.0.0",
  "description": "A Component Library for Vue 3",
  "keywords": [
    "material-template",
    "element",
    "component library",
    "ui framework",
    "ui",
    "vue"
  ],
  "homepage": "https://github.com/ywenhao/material-template",
  "bugs": {
    "url": "https://github.com/ywenhao/material-template/issues"
  },
  "license": "MIT",
  "main": "index.js",
  "module": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    },
    "./build-plugins": {
      "types": "./build-plugins/index.d.ts",
      "import": "./build-plugins/index.mjs"
    },
    "./global": {
      "types": "./global.d.ts"
    },
    "./*": "./*"
  },
  "unpkg": "dist/index.full.js",
  "jsdelivr": "dist/index.full.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ywenhao/material-template.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "style": "dist/index.css",
  "sideEffects": [
    "dist/*",
    "theme-chalk/**/*.css",
    "theme-chalk/src/**/*.scss",
    "es/components/*/style/*",
    "lib/components/*/style/*"
  ],
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "@ctrl/tinycolor": "catalog:",
    "@floating-ui/dom": "catalog:",
    "@types/lodash": "catalog:",
    "@types/lodash-es": "catalog:",
    "@vueuse/core": "catalog:",
    "async-validator": "catalog:",
    "dayjs": "catalog:",
    "escape-html": "catalog:",
    "lodash": "catalog:",
    "lodash-es": "catalog:",
    "lodash-unified": "catalog:",
    "memoize-one": "catalog:",
    "normalize-wheel-es": "catalog:"
  },
  "devDependencies": {
    "@types/node": "*",
    "vue": "catalog:",
    "vue-router": "catalog:"
  },
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all"
  ]
}
